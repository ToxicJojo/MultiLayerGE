// Proxy Protocols
protocol GetNodeCountProxy {
  Type: Syn;
  Request: StandardAlgorithmMessage;
  Response: void;
}

protocol GetEdgeCountProxy {
  Type: Syn;
  Request: StandardAlgorithmMessage;
  Response: void;
}

protocol GetAverageEdgeDegreeProxy {
  Type: Syn;
  Request: StandardAlgorithmMessage;
  Response: void;
}


