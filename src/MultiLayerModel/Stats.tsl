protocol GetNodeCountServer {
  Type: Asyn;
  Request: void;
  Response: void;
}

protocol GetEdgeCountServer {
  Type: Asyn;
  Request: void;
  Response: void;
}

protocol GetAverageEdgeDegreeServer {
  Type: Asyn;
  Request: void;
  Response: void;
}
